{
  "compilerOptions": {
      "sourceMap": true,
      "module": "commonjs",
      "strict": true,
      "noEmit": true,
      "target": "esnext",
      "jsx": "react",
      "allowJs": true,
      "lib": ["dom", "es6", "esnext"],
      "isolatedModules": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noUnusedLocals": false
  },
  "exclude": ["./node_modules", "./public", "./.cache"],
  "include": [
      "./src/**/*", "src/state/provider.tsx", "gatsby-ssr.ts", "src/state/AppContext.tsx"
  ]
}